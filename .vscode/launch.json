{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (dotnet run)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "dotnet",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/SystatusMonitor/SystatusMonitor.csproj",
                "--no-build"
            ],
            "cwd": "${workspaceFolder}/SystatusMonitor",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": ".NET Core Launch (SystatusMonitor)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/SystatusMonitor/bin/Debug/net8.0-windows/SystatusMonitor.exe",
            "args": [],
            "cwd": "${workspaceFolder}/SystatusMonitor/bin/Debug/net8.0-windows",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}
